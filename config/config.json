{
  "application": "Achilles",
  "application_id": 1,
  "http_port": 8080,
  "log_config": {
    "log_in_terminal_over_file": true,
    "enable_indentation": true,
    "log_file_path": "log/",
    "log_max_size_mb": 10,
    "log_max_backups": 5,
    "log_max_age_days": 28
  },
  "http_config": {
    "timeout_in_ms": "2000ms",
    "hystrix_command": "default",
    "circuit_breaker_active_time_in_ms": 5000,
    "max_concurrent": 10,
    "error_threshold": 50,
    "retry_max": 2,
    "retry_backoff_in_ms": "100ms",
    "retry_jitter_in_ms": "100ms",
    "retry_max_duration": "1000ms",
    "request_to_volume_threshold": 5
  }
}